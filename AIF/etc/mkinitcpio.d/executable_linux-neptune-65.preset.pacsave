# mkinitcpio preset file for the 'linux-neptune-65' package

#ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-neptune-65"

PRESETS=('default')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-neptune-65.img"
#default_uki="/efi/EFI/Linux/arch-linux-neptune-65.efi"
#default_options="--splash /usr/share/systemd/bootctl/splash-arch.bmp"

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-neptune-65-fallback.img"
#fallback_uki="/efi/EFI/Linux/arch-linux-neptune-65-fallback.efi"
fallback_options="-S autodetect"
