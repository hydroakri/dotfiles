Host github.com
    # ProxyCommand          nc -X connect -x 127.0.0.1:1080 %h %p
    ServerAliveInterval   10
    Hostname ssh.github.com
    Port 443

Host *
    # ForwardAgent yes # open only in trusted machine
    AddKeysToAgent yes
    ControlMaster auto
    ControlPath /tmp/ssh_mux_%u_%C
    ControlPersist 30m
  
    # For Hardware Security Key
    IdentitiesOnly yes
